{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"outDir": "out",
		"rootDir": "src",
		"lib": [
			"ES2022"
		],
		"sourceMap": true,
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true,
		"useUnknownInCatchVariables": false
	},
	"exclude": ["test/*"]
}
